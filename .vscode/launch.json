{
    // Используйте IntelliSense, чтобы узнать о возможных атрибутах.
    // Наведите указатель мыши, чтобы просмотреть описания существующих атрибутов.
    // Для получения дополнительной информации посетите: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug sql example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "cwd": "examples/sql",
            "args": ["-type", "Entity", "-in", "../sql_util/postgres.go", "-out", "entity_sql.go", "-GetFieldValuesByTag", "db", "-const", "_upsert", "-const", "_selectByID", "-const", "_deleteByID", "-const", "_pk"]
        },
        {
            "name": "Debug squirrel example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "cwd": "examples/squirrel",
            "args": ["-type", "Entity", "-out", "entity_fields.go", "-wrap", "-Strings", "-TagValues", "db", "-GetFieldValuesByTag_", "-GetFieldValuesByTag", "db"]
        },
        {
            "name": "Debug gorm example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "cwd": "examples/gorm",
            "args": ["-type", "Entity", "-out", "entity_fields.go", "-enum-field-const", "{{(join type.name field.name) | snake | toUpper}}={{.gorm | rexp \"column:(\\\\w+),?\" | or field.name | snake | toUpper}}"]
        },        
        {
            "name": "Debug gorm json example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "cwd": "examples/gorm",
            "args": ["-type", "Entity", "-out", "entity_fields.go", "-enum-field-const", "rexp \"(?P<v>\\w+),\" .json"]
        },                            
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        }
    ] 
}